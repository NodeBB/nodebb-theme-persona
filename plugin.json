{
	"id": "nodebb-theme-persona-netdata",
	"library": "library.js",
	"hooks": [
		{
			"hook": "filter:teasers.get",
			"method": "getTeasers"
		},
		{
			"hook": "filter:widgets.getAreas",
			"method": "defineWidgetAreas"
		},
		{
			"hook": "filter:config.get",
			"method": "getThemeConfig"
		},
		{
			"hook": "static:app.load",
			"method": "init"
		},
		{
			"hook": "filter:admin.header.build",
			"method": "addAdminNavigation"
		},
		{
			"hook": "filter:topic.build",
			"method": "addUserToTopic"
		}
	],
	"scripts": [
		"lib/persona.js",
		"lib/modules/nprogress.js",
		"lib/modules/autohidingnavbar.min.js",
		"lib/modules/quickreply.js"
	],
	"modules": {
		"pulling.js": "node_modules/pulling/build/pulling-drawer.js"
	},
	"acpScripts": [
		"lib/admin.js"
	],
	"languages": "languages"
}